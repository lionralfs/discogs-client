{
    "name": "disconnect",
    "description": "A full-featured Discogs API v2.0 client library",
    "version": "1.2.2",
    "keywords": [
        "discogs",
        "api",
        "client",
        "oauth"
    ],
    "homepage": "https://github.com/bartve/disconnect",
    "bugs": "https://github.com/bartve/disconnect/issues",
    "license": "MIT",
    "author": "Bart van Eijck <voodoo107@hotmail.com>",
    "main": "lib/index.js",
    "module": "lib/index.js",
    "type": "module",
    "directories": {
        "lib": "./lib"
    },
    "types": "types/index.d.ts",
    "repository": {
        "type": "git",
        "url": "https://github.com/bartve/disconnect.git"
    },
    "scripts": {
        "test": "ava",
        "coverage": "c8 --reporter html ava",
        "prettier": "prettier --check **/**.{js,md,json}",
        "declaration:build": "rm -rf types && tsc -p tsconfig.json",
        "build": "rm -f commonjs/*.js && esbuild lib/index.js --format=cjs --bundle --platform=node --outfile=commonjs/index.js --target=node12",
        "prepare": "husky install"
    },
    "dependencies": {
        "node-fetch": "^3.2.0",
        "oauth-1.0a": "2.x.x"
    },
    "devDependencies": {
        "ava": "^4.0.1",
        "c8": "^7.11.0",
        "esbuild": "^0.14.11",
        "husky": "^7.0.4",
        "lint-staged": "^12.1.7",
        "msw": "^0.36.3",
        "prettier": "^2.5.1",
        "typescript": "^4.5.4"
    },
    "engines": {
        "node": ">=12"
    },
    "lint-staged": {
        "*.{js,md,json}": "prettier --write"
    }
}
